{
  "name": "@tsfga/kysely",
  "version": "0.1.0",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": ["dist", "src"],
  "scripts": {
    "build": "tsup",
    "db:generate": "bunx kysely-codegen --out-file src/schema.ts",
    "db:latest": "kysely migrate:latest",
    "db:rollback": "kysely migrate:rollback --all",
    "test": "bun test",
    "tsc": "tsc --noEmit"
  },
  "peerDependencies": {
    "@tsfga/core": "workspace:*",
    "kysely": ">=0.27.0",
    "pg": ">=8.0.0"
  },
  "devDependencies": {
    "@tsfga/core": "workspace:*",
    "@types/bun": "1.3.9",
    "@types/pg": "8.16.0",
    "kysely": "0.28.11",
    "kysely-codegen": "0.20.0",
    "kysely-ctl": "0.20.0",
    "pg": "8.18.0",
    "tsup": "8.5.1",
    "typescript": "5.9.3"
  }
}
